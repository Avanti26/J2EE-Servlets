<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>HttpServletApplication</display-name>
  <welcome-file-list>
    <welcome-file>welcome.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>http1</servlet-name>
  <servlet-class>servletapp.org.HttpDemo1</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>http1</servlet-name>
  <url-pattern>/courselink</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>http2</servlet-name>
  <servlet-class>servletapp.org.HttpDemo2</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>http2</servlet-name>
  <url-pattern>/demolink</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>http3</servlet-name>
  <servlet-class>servletapp.org.HttpDemo3</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>http3</servlet-name>
  <url-pattern>/getbill</url-pattern>
  </servlet-mapping>
</web-app>